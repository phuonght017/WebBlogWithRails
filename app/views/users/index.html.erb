<h1>USERS/INDEX_TABLE INFORMATION OF ALL USERS</h1>
<table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Created at</th>
            <th>Updated at</th>
            <th></th>
            <th></th>
        </tr>
        <% @users.each do |user|%>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.created_at %></td>
            <td><%= user.updated_at %></td>
            <td><%= link_to 'Edit', edit_user_path(user) %></td>
        </tr>
        <% end %>
</table>